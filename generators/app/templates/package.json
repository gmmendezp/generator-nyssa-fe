{
  "name": "<%= name %>",
  "version": "1.0.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "clean": "rimraf build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "standard --verbose | snazzy",
    "format": "prettier --single-quote --no-semi --write \"**/*.js\" && standard --fix",
    "precommit": "lint-staged"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --no-semi --write",
      "standard --fix",
      "git add"
    ]
  },
  "dependencies": {
    "csstips": "^0.2.2",
    "csx": "^8.5.0",
    "intl": "^1.2.5",
    "react": "^15.6.1",
    "react-md": "^1.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-observable": "^0.16.0",
    "typestyle": "^1.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "enzyme": "^2.9.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "prettier": "^1.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.13",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  }
